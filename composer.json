{
  "name": "spryker/security",
  "description": "Security module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/application-extension": "^1.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/security-extension": "^1.0.0",
    "spryker/symfony": "^3.3.3"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/event-dispatcher": "*",
    "spryker/event-dispatcher-extension": "*",
    "spryker/router": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/container": "Add this module to use the Container.",
    "spryker/event-dispatcher": "Add this module to use the EventDispatcher.",
    "spryker/event-dispatcher-extension": "Add this module to use the EventDispatcherExtension."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/",
      "SprykerTest\\Yves\\Security\\Fixtures\\": "tests/SprykerTest/Yves/Security/_support/Fixtures/",
      "SprykerTest\\Yves\\Security\\Helper\\": "tests/SprykerTest/Yves/Security/_support/Helper/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
